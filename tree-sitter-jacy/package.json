{
  "name": "tree-sitter-jacy",
  "version": "0.0.2-rc.2",
  "description": "Tree-Sitter grammar for Jacy",
  "main": "grammar.js",
  "scripts": {
    "build": "export PATH=\"./node_modules/tree-sitter:$PATH\" && tree-sitter generate && node-gyp build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hazer-hazer/Jacy-Atom.git"
  },
  "keywords": [
    "tree-sitter",
    "jacy",
    "jacy-lang",
    "programming-language",
    "syntax",
    "parser"
  ],
  "author": "hazer-hazer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hazer-hazer/Jacy-Atom/issues"
  },
  "homepage": "https://github.com/hazer-hazer/Jacy-Atom#readme",
  "devDependencies": {
    "tree-sitter-cli": "0.19.5"
  },
  "dependencies": {
    "nan": "^2.12.1"
  },
  "tree-sitter": [{
    "scope": "source.jc",
    "file-types": ["jc"],
    "injection-regex": "^(jc|jacy)$"
  }]
}
