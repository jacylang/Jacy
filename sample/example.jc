mod mood {
    type Type = i32;

    enum Enum {
        Variant(Type),
    }

    pub struct Struct {
        field: Enum::Variant,
    }

    func foo(param: Type) {
        print(param);
    }

    trait Trait {
        func baz: Type;
    }

    impl Struct {
        func bar {
            foo(self.field);
        }
    }
}

func main {
    use mood::Struct;

    let s: Struct = {field: 100};

    s.baz();
}
